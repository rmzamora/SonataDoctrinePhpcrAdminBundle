{
    "name": "rmzamora/doctrine-phpcr-admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata / Integrate Doctrine PHPCR into the SonataAdminBundle, rmzamora forked",
    "keywords": ["Admin Generator", "admin", "sonata", "bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "mell m. zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Nacho Martin",
            "email": "nacho.martin@gmail.com",
            "homepage": "http://nacho-martin.com"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDoctrinePHPCRAdminBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rmzamora/admin-bundle": "2.3.*@dev",
        "symfony/framework-bundle": "~2.2",
        "symfony/property-access": "~2.2",
        "doctrine/phpcr-odm": "~1.1",
        "doctrine/phpcr-bundle": "~1.1",
        "symfony-cmf/tree-browser-bundle": "~2.0",
        "symfony-cmf/resource-rest-bundle": "~1.0"
    },
    "require-dev": {
        "jackalope/jackalope-jackrabbit": "~1.0"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Sonata\\DoctrinePHPCRAdminBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
